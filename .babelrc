{
	"plugins": [
		[
			"module-resolver", {
			"root": ["./"],
			"alias": {
				"styles": "./styles"
			},
			"cwd": "babelrc"
		}],
		[
			"wrap-in-js",
			{
				"extensions": ["css$", "scss$"]
			}
		]
	],
	"presets": [
		["env",
			{
				"targets": {
					"browsers": ["last 2 versions", "safari >= 7", "ie >= 7"]
				}
			}
		],
		"next/babel",
		"react",
		"stage-0"
	],
	"ignore": []
}
